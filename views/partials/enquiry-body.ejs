    <div id="page-wrapper">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Welcome! Engineer</h1>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- <div class="row" style="padding-bottom: 10px;">
            <div class="col-lg-12">
                <a href="/addenquiry" class="btn btn-success btn-lg"><i class="fa fa-plus fa-fw"></i>Add New Enquiry</a>
                </div>
        </div> -->
        <!-- /.row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <div class="row">
                            <div class="col-sm-8">
                                <h4>
                                Enquiry Log</h4>
                            </div>
                            <div class="col-sm-4">
                            <a href="/addenquiry" class="btn btn-lg" style="float:right ;"><i class="fa fa-plus fa-fw"></i>Add New Enquiry</a>
                        </div>
                        </div>
                            
                    </div>
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        <%if (items.length > 0) { %>
                        <table width="100%" class="table table-striped table-bordered table-hover" id="enquiry_table">
                            <thead>
                                <tr>
                                        
                                    <th>Job Ref</th>
                                    <th>Date</th>
                                    <th>Project</th>
                                    <th>Customer</th>
                                    <th>Product</th>
                                    <th>Qty</th>
                                    <th>Amount</th>
                                    <th>Sales Person</th>
                                    <th>Status</th>
                                    <th colspan="2">Actions</th>
                                </tr>
                            </thead>
                            <tbody class="table-bordered">
                                <% items.forEach(function(item, index){ %>
                                    <tr>
                                           
                                        <td><%= item.job_ref %></td>
                                        <td><%= item.reci_date %></td>
                                        <td><%= item.project %></td>
                                        <td><%= item.customer %></td>
                                        <td><%= item.product %></td>
                                        <td><%= item.qty %></td>
                                        <td><%= item.amount %></td>
                                        <td><%= item.sales_per %></td>
                                        <td><%= item.status %></td>
                                        <td colspan="2">
                                            <a href="/editenquiry/<%= item.job_ref %>" class="btn btn-warning">Edit</a>
                                            <a href="/viewenquiry/<%= item.job_ref %>" class="btn btn-info">View</a>
                                        </td>
                                    </tr>
                                    <% }); %>
                            </tbody>
                        </table>
                        
                            <%}%>
                        <!-- /.table-responsive -->

                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
    </div>
